          </direct-parameter>
          <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
              <cocoa key="shared_secret"/>
          </parameter>
          <parameter name="text" code="bTtE" description="The text you want to show in the menubar item" type="text" optional="yes">
              <cocoa key="text"/>
          </parameter>
          <parameter name="icon_path" code="bTiU" description="A file path to an icon that should be loaded" type="text" optional="yes">
              <cocoa key="icon_path"/>
          </parameter>
          <parameter name="icon_data" code="bTiC" description="The base64 encoded icon data of the icon you want to show on the menubar item" type="text" optional="yes">
              <cocoa key="icon_data"/>
          </parameter>
          <parameter name="sf_symbol_name" code="sfNA" description="The name of the sf symbol to load" type="text" optional="yes">
              <cocoa key="sf_symbol_name"/>
          </parameter>
          <parameter name="sf_symbol_size" code="sfSi" description="The size of the sf symbol to load" type="number" optional="yes">
              <cocoa key="sf_symbol_size"/>
          </parameter>
          <parameter name="sf_symbol_weight" code="sfWe" description="The weight of the sf symbol to load" type="number" optional="yes">
