        </parameter>
        <parameter name="subtitle" code="bsTi" description="The subtitle" type="text" optional="yes">
            <cocoa key="subTitle"/>
        </parameter>
        <parameter name="sound name" code="soNa" description="Sound name" type="text" optional="yes">
            <cocoa key="soundName"/>
        </parameter>
        <parameter name="image path" code="soIm" description="Image path" type="text" optional="yes">
            <cocoa key="imagePath"/>
        </parameter>
       
    </command>
    
    <command name="update_menubar_item" code="BtttBupM" description="Provide new content to a scriptable BTT Menu Bar Item without triggering the assigned script">
          <cocoa class="BTTAppleScriptHandler"/>
          <direct-parameter description="The uuid of Menu Bar Item you want to update. You can get it by right-clicking the item you set up in BetterTouchTool.">
