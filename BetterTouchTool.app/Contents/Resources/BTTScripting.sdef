<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd">

<dictionary title="BTT Terminology">
<suite name="BetterTouchTool Script Suite" code="bTtS" description="BetterTouchTool Script Suite.">
    <class name="application" code="capp" description="BTTs top level scripting object.">
        <cocoa class="BTTApplication"/>
    </class>
    
    <command name="get_location" code="BttGLocS" description="This returns the approximate location of your Mac.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="This returns the approximate location of your Mac.">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="The value of the variable"/>
        
    </command>
    
    <command name="chat_gpt" code="BttChaTG" description="Call ChatGPT">
        <cocoa class="BTTAppleScriptHandler"/>
        
      
        <parameter name="identifier" code="iDTf" description="An identifier to keep history" type="text" optional="yes">
            <cocoa key="identifier"/>
        </parameter>
        <parameter name="user" code="uSRq" description="The user request (what you ask chatgpt to do)" type="text" optional="yes">
            <cocoa key="user"/>
        </parameter>
        <parameter name="system" code="sSRq" description="Tell ChatGPT how it should behave" type="text" optional="yes">
            <cocoa key="system"/>
        </parameter>
        <parameter name="input" code="iSRq" description="Additional input for your request" type="text" optional="yes">
            <cocoa key="input"/>
        </parameter>
        <parameter name="maxHistory" code="mNRq" description="Max number of history items to include" type="number" optional="yes">
            <cocoa key="maxHistory"/>
        </parameter>
        
        <parameter name="apiKey" code="iSRq" description="API Key (will be saved, so you don't need to pass it every time)" type="text" optional="yes">
            <cocoa key="apiKey"/>
        </parameter>
        
        
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="The badge label"/>
        
    </command>
    
    
    <command name="get_dock_badge_for" code="BttGDocK" description="This retrieves the dock badge for the given application name.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the app">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="update_interval" code="bTuV" description="This defines how often the data that is returned by this function is updated inside BTT. If set to a negative value, it will not update anymore. Default is -1." type="number" optional="yes">
            <cocoa key="update_interval"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="The badge label"/>
        
    </command>
    
    <command name="is_app_running" code="BttIRunI" description="This allows you to find out whether the given app is currently running">
         <cocoa class="BTTAppleScriptHandler"/>
         
         <direct-parameter description="The name or bundle identifier of the app">
             <type type="text"/>
             
         </direct-parameter>
         <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
             <cocoa key="shared_secret"/>
         </parameter>
         <result type="boolean" description="The result"/>
         
     </command>
    
    <command name="set_persistent_string_variable" code="BtttSetP" description="This allows you to set a variable to a given string that persists over BTT relaunches.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the persistent variable you want to set">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="to" code="bTpV" description="The value of the persistent variable" type="text" optional="yes">
            <cocoa key="to"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    <command name="set_string_variable" code="BtttSetV" description="This allows you to set a variable to a given string for the runtime of BTT.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the variable you want to set">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="to" code="bTpV" description="The value of the variable" type="text" optional="yes">
            <cocoa key="to"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    <command name="set_persistent_number_variable" code="BtttSeNP" description="This allows you to set a variable to a given number that persists over BTT relaunches.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the persistent variable you want to set">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="to" code="bTpV" description="The value of the persistent variable" type="number" optional="yes">
            <cocoa key="to"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    <command name="set_number_variable" code="BtttSeNV" description="This allows you to set a variable to a given number for the runtime of BTT.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the variable you want to set">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="to" code="bTpV" description="The value of the variable" type="real" optional="yes">
            <cocoa key="to"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    

    <command name="get_active_touch_bar_group" code="BttGATBg" description="This allows you to get the currently active Touch Bar Group.">
        <cocoa class="BTTAppleScriptHandler"/>
        <result type="text" description="The value of the group"/>
    </command>
    
    <command name="get_string_variable" code="BttGVarS" description="This allows you to get the value of a previously set string variable in BTT.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the variable you want to get">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <parameter name="default" code="bTdf" description="The default value of the variable" type="text" optional="yes">
                 <cocoa key="default"/>
        </parameter>
        <result type="text" description="The value of the variable"/>
        
    </command>
    
    
    <command name="get_number_variable" code="BttGVarN" description="This allows you to get the value of a previously set string variable in BTT.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the variable you want to get">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="default" code="bDfV" description="The default value of the variable" type="real" optional="yes">
            <cocoa key="default"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="real" description="The value of the variable"/>
        
    </command>
    <command name="is_true_tone_enabled" code="BttTTenB" description="This returns true when true tone is currently enabled, false otherwise..">
        <cocoa class="BTTAppleScriptHandler"/>
      
        <result type="boolean" description="true when true tone is currently enabled, false otherwise"/>
        
    </command>
    
    <command name="trigger_named_async_without_response" code="BtttNaWr" description="This triggers one of the named triggers configured in the Other tab in BTT. It runs async and doesn't return a response.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the trigger.">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="delay" code="bTCC" description="The delay in seconds" type="real" optional="yes">
            <cocoa key="delay"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result is just a static value here"/>
        
    </command>

    <command name="cancel_delayed_named_trigger_execution" code="BtttNaDe" description="Cancels a delayed named trigger execution.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the trigger.">
            <type type="text"/>
            
        </direct-parameter>
    
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result is just a static value here"/>
        
    </command>

    <command name="trigger_named" code="BtttNaTr" description="This triggers one of the named triggers configured in the Other tab in BTT.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the trigger.">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    <command name="run_shortcut" code="BtttShOr" description="This triggers a shortcut from the Shortcuts app.">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The name of the Shortcut.">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    <command name="trigger_action" code="BtttActi" description="This triggers a BTT predefined action without adding it to BTT">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The uuid of the trigger (gesture, shortcut, touch bar item etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool. Example usage: tell application BetterTouchTool trigger uuid theUUID end tell">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>

    </command>
    
    <command name="get_trigger" code="BtttGeti" description="This returns the properties of the requested trigger">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The uuid of the trigger (gesture, shortcut, touch bar item etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool. Example usage: tell application BetterTouchTool trigger uuid theUUID end tell">
            <type type="text"/>
            
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Trigger description"/>

    </command>
    
    <command name="delete_triggers" code="BtttDelS" description="This deletes all triggers matching the provided values (test with the get_triggers function first)">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <parameter name="trigger_type" code="dtTt" description="The type of the trigger, e.g. BTTTriggerTypeMagicMouse" type="text" optional="yes">
            <cocoa key="trigger_type"/>
        </parameter>
        
        <parameter name="trigger_id" code="dtID" description="The id of the trigger, e.g. 123" type="number" optional="yes">
            <cocoa key="trigger_id"/>
        </parameter>
        
        
        <parameter name="trigger_uuid" code="dUID" description="The uuid of the trigger" type="text" optional="yes">
            <cocoa key="trigger_uuid"/>
        </parameter>
        
        
        <parameter name="trigger_app_bundle_identifier" code="daPp" description="The bundle identifier of the app you want to retrieve triggers for" type="text" optional="yes">
            <cocoa key="trigger_app_bundle_identifier"/>
        </parameter>
        
        <parameter name="preset" code="daGN" description="The preset you want to retrieve triggers for" type="text" optional="yes">
            <cocoa key="preset"/>
        </parameter>
        
        
        <parameter name="trigger_parent_uuid" code="dUID" description="The UUID of the parent group / trigger" type="text" optional="yes">
            <cocoa key="trigger_parent_uuid"/>
        </parameter>
        
        <parameter name="return_only_if_modifiers_match" code="dMOD" description="If set only triggers that match the pressed modifier keys will be returned." type="number" optional="yes">
            <cocoa key="return_only_if_modifiers_match"/>
        </parameter>

       
        <parameter name="shared_secret" code="dtSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Trigger description"/>

    </command>
    
    <command name="get_triggers" code="BtttGets" description="This returns all triggers matching the provided values">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <parameter name="trigger_type" code="btTt" description="The type of the trigger, e.g. BTTTriggerTypeMagicMouse" type="text" optional="yes">
            <cocoa key="trigger_type"/>
        </parameter>
        
        <parameter name="trigger_id" code="btID" description="The id of the trigger, e.g. 123" type="number" optional="yes">
            <cocoa key="trigger_id"/>
        </parameter>
        
        
        <parameter name="trigger_uuid" code="bUID" description="The uuid of the trigger" type="text" optional="yes">
            <cocoa key="trigger_uuid"/>
        </parameter>
        
        
        <parameter name="trigger_app_bundle_identifier" code="PaPp" description="The bundle identifier of the app you want to retrieve triggers for" type="text" optional="yes">
            <cocoa key="trigger_app_bundle_identifier"/>
        </parameter>
        
        <parameter name="trigger_parent_uuid" code="PUID" description="The UUID of the parent group / trigger" type="text" optional="yes">
            <cocoa key="trigger_parent_uuid"/>
        </parameter>
        
        <parameter name="return_only_if_modifiers_match" code="PMOD" description="If set only triggers that match the pressed modifier keys will be returned." type="number" optional="yes">
            <cocoa key="return_only_if_modifiers_match"/>
        </parameter>
        
        <parameter name="preset" code="daGN" description="The preset you want to retrieve triggers for" type="text" optional="yes">
            <cocoa key="preset"/>
        </parameter>

       
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Trigger description"/>

    </command>
    
    <command name="delete_trigger" code="BtttDelT" description="This will delete the trigger (Gesture, Shortcut, etc.) with the given UUID">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="The uuid of the trigger (gesture, shortcut, etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool. Example usage: tell application BetterTouchTool trigger uuid theUUID end tell">
            <type type="text"/>
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>
        
    </command>
    
    
    
    <command name="execute_assigned_actions_for_trigger" code="BtttTrig" description="Make any configured trigger execute its assigned actions">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="The uuid of the trigger (gesture, shortcut, etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool. Example usage: tell application BetterTouchTool trigger uuid theUUID end tell">
            <type type="text"/>
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="Possible action result"/>

    </command>
    
    
    <command name="add_new_trigger" code="BtttAddt" description="Add a new BetterTouchTool trigger via JSON input">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="This must be a JSON dictionary string which can contain all keys and properties that are allowed in BTT's JSON export. You can get them for a specific gesture/trigger by right-clicking it and choosing 'Copy JSON'">
            <type type="text"/>
            
        </direct-parameter>
        
        
        <parameter name="parent_uuid" code="bpaR" description="If you want to add the trigger to a group, specify the group uuid as parent here." type="text" optional="yes">
            <cocoa key="parent_uuid"/>
        </parameter>
        
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <result type="text" description="The uuid of the created trigger"/>

    </command>
    
    <command name="update_trigger" code="BtttUpdt" description="Update the properties of any BetterTouchTool trigger. ">
        <cocoa class="BTTAppleScriptHandler"/>
        
        <direct-parameter description="The uuid of the trigger (gesture, shortcut, etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool.">
            <type type="text"/>
        </direct-parameter>
        
        <parameter name="json" code="btJS" description="This must be a JSON dictionary string which can contain all keys and properties that are allowed in BTT's JSON export. You can get them for a specific gesture/trigger by right-clicking it and choosing 'Copy JSON'" type="text">
            <cocoa key="json"/>
        </parameter>
        <parameter name="trigger_parent_uuid" code="xUID" description="The UUID of the parent group / trigger" type="text" optional="yes">
            <cocoa key="trigger_parent_uuid"/>
        </parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        
        <result type="text" description="The uuid of the created or updated trigger"/>

    </command>


    
    <command name="refresh_widget" code="BtttRefr" description="Run a Touch Bar widget's assigned script to update the widget.">
        <cocoa class="BTTAppleScriptHandler"/>

        <direct-parameter description="The uuid of the trigger (gesture, shortcut, etc.). You can get it by right-clicking a trigger you set up in BetterTouchTool.">
            <type type="text"/>
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
    </command>
    
    
    <command name="update_touch_bar_widget" code="BtttBupd" description="Provide new content to a Touch Bar script widget without triggering the script">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="The uuid of Touch Bar widget you want to update. You can get it by right-clicking the widget you set up in BetterTouchTool.">
            <type type="text"/>
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <parameter name="text" code="bTtE" description="The text you want to show in the widget" type="text" optional="yes">
            <cocoa key="text"/>
        </parameter>
        <parameter name="icon_path" code="bTiU" description="A file path to an icon that should be loaded" type="text" optional="yes">
            <cocoa key="icon_path"/>
        </parameter>
        <parameter name="sf_symbol_name" code="sfNA" description="The name of the sf symbol to load" type="text" optional="yes">
            <cocoa key="sf_symbol_name"/>
        </parameter>
        <parameter name="sf_symbol_size" code="sfSi" description="The size of the sf symbol to load" type="number" optional="yes">
            <cocoa key="sf_symbol_size"/>
        </parameter>
        <parameter name="sf_symbol_weight" code="sfWe" description="The weight of the sf symbol to load" type="number" optional="yes">
            <cocoa key="sf_symbol_weight"/>
        </parameter>
        <parameter name="icon_data" code="bTiC" description="The base64 encoded icon data of the icon you want to show on the Touch Bar widget" type="text" optional="yes">
            <cocoa key="icon_data"/>
        </parameter>
        <parameter name="background_color" code="btCo" description="The background color the widget should have. Must be in this format (RGBA): 255,255,255,255" type="text" optional="yes">
            <cocoa key="background_color"/>
        </parameter>
    </command>
    
    <command name="update_stream_deck_widget" code="BtttStpd" description="Provide new content to a Stream Deck script widget without triggering the script">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="The uuid of Stream Deck widget you want to update. You can get it by right-clicking the widget you set up in BetterTouchTool.">
            <type type="text"/>
        </direct-parameter>
        <parameter name="shared_secret" code="btSs" description="You can specify a shared secret which must be contained in all Apple Script calls in the advanced BTT preferences." type="text" optional="yes">
            <cocoa key="shared_secret"/>
        </parameter>
        <parameter name="text" code="bTtE" description="The text you want to show in the widget" type="text" optional="yes">
            <cocoa key="text"/>
        </parameter>
        <parameter name="json" code="bTiU" description="A JSON string containing all the properties you want to update" type="text" optional="yes">
            <cocoa key="json"/>
        </parameter>
       
    </command>
    
    <command name="display_notification" code="BtttDiNo" description="Display a notification in Notification Center">
        <cocoa class="BTTAppleScriptHandler"/>
        <direct-parameter description="Notification body">
            <type type="text"/>
        </direct-parameter>
        <parameter name="with title" code="bwTi" description="Title" type="text" optional="yes">
